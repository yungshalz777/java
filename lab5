import static java.lang.StrictMath.pow;

interface Solid {
    double getVolume();            // Метод для отримання об'єму
    void setVolume(double volume); // Можна реалізувати за потреби
}

// Клас Піраміда
class Pyramid implements Solid {
    private double a; // довжина сторони основи
    private double h; // висота

    Pyramid(double a, double h) {
        this.a = a;
        this.h = h;
    }

    // Сеттери
    public void setA(double a) { this.a = a; }
    public void setH(double h) { this.h = h; }

    // Геттери
    public double getA() { return a; }
    public double getH() { return h; }

    @Override
    public double getVolume() {
        // Об'єм правильно: (1/3) * площа основи * висота
        // Припустимо, основа квадрат: площа = a^2
        return (1.0 / 3.0) * pow(a, 2) * h;
    }

    // Порівняння об'ємів (з точністю eps)
    public boolean isEqual(Pyramid p) {
        double eps = 1e-6;
        return Math.abs(this.getVolume() - p.getVolume()) < eps;
    }

    // setVolume для реалізації інтерфейсу (необов'язково)
    @Override
    public void setVolume(double volume) {
        // Можна обчислити h з відомого об'єму: V = a^2*h/3 => h = 3*V/a^2
        this.h = (3 * volume) / pow(a, 2);
    }
}

// Клас Куля
class Sphere implements Solid {
    private double r; // радіус

    Sphere(double r) { this.r = r; }

    public void setR(double r) { this.r = r; }
    public double getR() { return r; }

    @Override
    public double getVolume() {
        return (4.0 / 3.0) * Math.PI * pow(r, 3); // використовуємо Math.PI
    }

    public boolean isEqual(Sphere s) {
        double eps = 1e-6;
        return Math.abs(this.getVolume() - s.getVolume()) < eps;
    }

    @Override
    public void setVolume(double volume) {
        // Обчислюємо r з відомого об'єму: V = 4/3 * pi * r^3 => r = (3*V/(4*pi))^(1/3)
        this.r = pow((3 * volume) / (4 * Math.PI), 1.0/3.0);
    }
}
