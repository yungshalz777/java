import static java.lang.StrictMath.pow;

interface Solid {
    double getVolume();
    void setVolume(double volume);
}

// Клас Піраміда
class Pyramid implements Solid {
    private double a;
    private double h;

    Pyramid(double a, double h) {
        this.a = a;
        this.h = h;
    }

    public void setA(double a) { this.a = a; }
    public void setH(double h) { this.h = h; }
    public double getA() { return a; }
    public double getH() { return h; }

    @Override
    public double getVolume() {
        return (1.0 / 3.0) * pow(a, 2) * h;
    }

    public boolean isEqual(Pyramid p) {
        double eps = 1e-6;
        return Math.abs(this.getVolume() - p.getVolume()) < eps;
    }

    @Override
    public void setVolume(double volume) {
        this.h = (3 * volume) / pow(a, 2);
    }
}

// Клас Куля
class Sphere implements Solid {
    private double r;

    Sphere(double r) { this.r = r; }
    public void setR(double r) { this.r = r; }
    public double getR() { return r; }

    @Override
    public double getVolume() {
        return (4.0 / 3.0) * Math.PI * pow(r, 3);
    }

    public boolean isEqual(Sphere s) {
        double eps = 1e-6;
        return Math.abs(this.getVolume() - s.getVolume()) < eps;
    }

    @Override
    public void setVolume(double volume) {
        this.r = pow((3 * volume) / (4 * Math.PI), 1.0/3.0);
    }
}

// Головний клас для запуску
public class lab5 {
    public static void main(String[] args) {
        Pyramid p1 = new Pyramid(3, 5);
        Pyramid p2 = new Pyramid(3, 5);
        Sphere s1 = new Sphere(2);
        Sphere s2 = new Sphere(2);

        System.out.println("Об'єм піраміди p1: " + p1.getVolume());
        System.out.println("Об'єм піраміди p2: " + p2.getVolume());
        System.out.println("Піраміди рівні: " + p1.isEqual(p2));

        System.out.println("Об'єм кулі s1: " + s1.getVolume());
        System.out.println("Об'єм кулі s2: " + s2.getVolume());
        System.out.println("Кулі рівні: " + s1.isEqual(s2));
    }
}
